<div class="col-md-6 col-md-offset-3">

  <div class="edit-review-form">
    <h1>Értékelés szerkesztése</h1>

    <%= simple_form_for([@szaki, @review]) do |f| %>
      <div id="rating-form">
        <label>Értékelés</label>
      </div>
      <label><%= f.input :comment %></label>

      <%= f.button :submit, :class => "btn-custom2" %>
    <% end %>
  </div>

</div>

<script>
    $('#rating-form').raty({
        path: '/assets/',
        scoreName: 'review[rating]'
    });
</script>